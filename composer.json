{
    "name": "zenstruck/porpaginas",
    "description": "Library that generically solves several pagination issues with DAO/repository abstractions.",
    "keywords": ["page", "paging", "paginator", "pagination"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Eberlei",
            "email": "kontakt@beberlei.de"
        },
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Zenstruck\\Porpaginas\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Porpaginas\\Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "pagerfanta/pagerfanta": "^1.0",
        "phpunit/phpunit": "^7.5",
        "ocramius/doctrine-batch-utils": "^2.0"
    },
    "suggest": {
        "doctrine/orm": "For integration with Doctrine ORM",
        "doctrine/dbal": "For integration with Doctrine DBAL",
        "ocramius/doctrine-batch-utils": "For batch processing ORMQueryResult's",
        "pagerfanta/pagerfanta": "For rendering pagination with Pagerfanta"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
